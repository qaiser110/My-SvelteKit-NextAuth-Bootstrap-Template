{
	"name": "authjs-firebase-svelte",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev --port 5173 --open",
		"db:open": "drizzle-kit studio --port 3333",
		"db:push": "drizzle-kit push:sqlite",
		"drizzle:studio": "drizzle-kit studio --port 3333",
		"turso:local": "turso dev --db-file at-the-polls.db --port 5555",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write ."
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@types/eslint": "8.56.0",
		"@typescript-eslint/eslint-plugin": "^6.0.0",
		"@typescript-eslint/parser": "^6.0.0",
		"dotenv": "^16.3.1",
		"drizzle-kit": "^0.20.10",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.35.1",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.1.2",
		"svelte": "^4.2.7",
		"svelte-check": "^3.6.0",
		"tslib": "^2.4.1",
		"tsx": "^4.7.0",
		"typescript": "^5.0.0",
		"vite": "^5.0.3"
	},
	"type": "module",
	"dependencies": {
		"@auth/core": "^0.20.0",
		"@auth/drizzle-adapter": "^0.3.14",
		"@auth/sveltekit": "^0.5.2",
		"@libsql/client": "0.4.0-pre.7",
		"drizzle-orm": "^0.29.3"
	}
}
